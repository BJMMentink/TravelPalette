<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Test Page</title>
</head>
<body>
    <h1>Test Page</h1>

    <div id="feature-info"></div>

    <script>
        // Function to extract query parameters from URL
        function getParameterByName(name, url) {
            if (!url) url = window.location.href;
            name = name.replace(/[\[\]]/g, '\\$&');
            var regex = new RegExp('[?&]' + name + '(=([^&#]*)|&|#|$)'),
                results = regex.exec(url);
            if (!results) return null;
            if (!results[2]) return '';
            return decodeURIComponent(results[2].replace(/\+/g, ' '));
        }

        // Retrieve the serialized feature object from the URL
        var serializedFeature = getParameterByName('feature');
        if (serializedFeature) {
            // Parse the serialized feature object back to JSON
            var feature = JSON.parse(decodeURIComponent(serializedFeature));
            // Display all properties of the feature on the page
            var featureInfoElement = document.getElementById('feature-info');
            var html = '<h2>Feature Information</h2><ul>';
            for (var key in feature.properties) {
                html += `<li><strong>${key}:</strong> ${feature.properties[key]}</li>`;
            }
            html += '</ul>';
            featureInfoElement.innerHTML = html;
        } else {
            // Handle case when feature object is not provided in the URL
            var featureInfoElement = document.getElementById('feature-info');
            featureInfoElement.innerHTML = '<p>No feature data provided.</p>';
        }
    </script>
</body>
</html>
